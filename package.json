{
  "name": "spin-wheel-app",
  "version": "1.0.0",
  "description": "A beautiful and interactive single-page spin wheel web application built with React Native",
  "main": "index.web.js",
  "scripts": {
    "start": "npx react-native start",
    "web": "npx react-native-web start",
    "build": "npx react-native-web build",
    "build:web": "npm install --legacy-peer-deps --include=dev && ./node_modules/.bin/webpack --config webpack.config.js --mode production",
    "build:gh-pages": "npm run build:web",
    "render-postbuild": "npm run build:web",
    "render-start": "npx serve -s build -l $PORT",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@reduxjs/toolkit": "^1.9.0",
    "ajv": "^8.17.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "buffer": "^6.0.3",
    "express": "^4.18.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.72.0",
    "react-native-confetti-cannon": "^1.5.0",
    "react-native-haptic-feedback": "^2.1.0",
    "react-native-reanimated": "^3.5.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^13.14.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.19.0",
    "react-redux": "^8.1.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.0.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "metro-config": "^0.76.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "react-native-svg-transformer": "^1.1.0",
    "serve": "^14.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.0"
  },
  "keywords": [
    "react-native",
    "spin-wheel",
    "web-app",
    "game",
    "animation"
  ],
  "author": "Swapnil Tandel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SwapnilTandel/spin-wheel-app.git"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
